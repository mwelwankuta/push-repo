{
  "name": "push-repo",
  "version": "1.0.14",
  "description": "Quickly create a local git project connected to github",
  "type": "module",
  "main": "./lib/index.js",
  "bin": "./lib/index.js",
  "scripts": {
    "start": "node --trace-warnings ./lib/index.js",
    "dev": "nodemon ./src/index.ts",
    "dev:start": "ts-node ./src/index.ts",
    "build": "tsc --project ./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mwelwankuta/start-repo.git"
  },
  "keywords": [
    "git",
    "init",
    "initialize",
    "repository",
    "repo",
    "github",
    "project",
    "create"
  ],
  "author": "Mwelwa Nkuta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mwelwankuta/start-repo/issues"
  },
  "publishConfig": {
    "access": "public",
    "branches": [
      "main"
    ]
  },
  "homepage": "https://github.com/mwelwankuta/start-repo#readme",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.15.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "chalk": "^5.2.0",
    "express": "^4.18.2",
    "minimist": "^1.2.8"
  }
}
